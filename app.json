{
  "expo": {
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "name": "lexi",
    "slug": "lexi",
    "owner": "hanxngn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/lexi_icon.png",
    "scheme": "com.hanxngn.lexi",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/lexi_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F0F6FF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hanxngn.lexi",
      "splash": {
        "image": "./assets/images/lexi_icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#F0F6FF"
      },
      "config": {
        "googleMapsApiKey": "AIzaSyDGa8GbaC9fsSExYXOe1oFOzOOCAh5SCT4"
      },
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSAllowsArbitraryLoadsInWebContent": true,
          "NSExceptionDomains": {
            "10.155.6.92": {
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSExceptionMinimumTLSVersion": "1.0",
              "NSExceptionRequiresForwardSecrecy": false,
              "NSIncludesSubdomains": true,
              "NSExceptionAllowsInsecureHTTPSLoads": true
            },
            "10.155.7.118": {
              "NSExceptionAllowsInsecureHTTPLoads": true,
              "NSExceptionMinimumTLSVersion": "1.0",
              "NSExceptionRequiresForwardSecrecy": false,
              "NSIncludesSubdomains": true,
              "NSExceptionAllowsInsecureHTTPSLoads": true
            }
          }
        },
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to upload images for workspace responses.",
        "NSCameraUsageDescription": "This app needs access to your camera to capture images for workspace responses.",
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location to show your position on the map.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location to show your position on the map."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/lexi_icon.png",
        "backgroundColor": "#F0F6FF"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.hanxngn.lexi",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDGa8GbaC9fsSExYXOe1oFOzOOCAh5SCT4"
        }
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Lexi to use your location."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "This app needs access to your photos to let you upload images for workspace responses.",
          "cameraPermission": "This app needs access to your camera to capture images for workspace responses."
        }
      ],
      "expo-web-browser",
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "baseUrl": "http://10.155.10.90:8081",
      "jwtSecret": "expo_is_the_best",
      "router": {},
      "serverUrl": "http://10.155.7.118:5000",
      "eas": {
        "projectId": "1fa1d659-5a94-4538-a713-a98ef81dec10"
      }
    }
  }
}
